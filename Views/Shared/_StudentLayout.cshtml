                            <a class="nav-link "asp-controller="Student" asp-action="MySchedule">Мій розклад</a>
                            <a class="nav-link " asp-controller="Task" asp-action="Tasks">Завдання</a>
                            <a class="nav-link" asp-controller="Test" asp-action="Create">Вправа</a>
                            
                        </div>
                    </div>
                </div>
            </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <footer >
            <div class="divfooter">
                <div class="divfootsect">
                     <h4 class="h4footer">Загальне</h4>
                    <ul class="ulfooter">

                        <li><a>Корисні посилання</a></li>
